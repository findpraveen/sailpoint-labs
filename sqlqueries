select A.ID, app.name as appName, ExtractValue(A.attributes, '/Attributes/Map/entry[@key="last_login"]/@value') as lastLogin from spt_link A, spt_application app where app.id = A.application and ExtractValue(A.attributes, '/Attributes/Map/entry[@key="last_login"]/@value')="2024-03-27" and (select count(*) from spt_link A, spt_application app where app.id=A.application and ExtractValue(A.attributes, '/Attributes/Map/entry[@key="last_login"]/@value')="2024-03-27")>=2;
